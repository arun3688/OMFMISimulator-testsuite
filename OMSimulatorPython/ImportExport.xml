<?xml version="1.0" encoding="UTF-8"?>
<Model>
	<SubModels>
		<SubModel Name="adder" ModelFile="../FMUs/me_adder1.fmu" solver="euler" />
		<SubModel Name="sourceA" ModelFile="../FMUs/me_source1.fmu" solver="euler">
			<ModelParams Name="A" Value="0.5" />
			<ModelParams Name="omega" Value="2" />
		</SubModel>
		<SubModel Name="sourceB" ModelFile="../FMUs/me_source1.fmu" solver="euler" />
	</SubModels>
	<Connections>
		<Connection From="sourceB.y" To="adder.x2" />
		<Connection From="sourceA.y" To="adder.x1" />
	</Connections>
	<SimulationParams StartTime="0.000000" StopTime="10.000000" tolerance="0.000010" communicationInterval="0.100000" variableFilter=".*" />
</Model>
